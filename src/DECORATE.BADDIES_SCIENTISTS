	TNT1 A 0 
	TNT1 A 0 Thing_ChangeTID(0, 0) //Remove BADDIES_TID so NightMorph won't resurrect us
 	TNT1 A 0 A_SpawnItemEx("ZombieScientistLoot", 0, 32, 0, 0, 0, 0, 0, 0, 0) 
    SCZH H 5
    SCZH I 5 A_Scream
    SCZH J 5 A_NoBlocking
    SCZH K 5
    SCZH L 5
    SCZH M 5
    //SCZH N -1
	SCZH N 0 A_QueueCorpse //Queue the corpse for removal
	//Remove ourselves after 60 seconds
	SCZH N 2100
	TNT1 A 0 A_SetTranslucent(1.0, 2)
	SCZH N 6 A_SetTranslucent(0.8, 2)
	SCZH N 6 A_SetTranslucent(0.6, 2)
	SCZH N 6 A_SetTranslucent(0.4, 2)
	SCZH N 6 A_SetTranslucent(0.2, 2)
	SCZH N 6 A_SetTranslucent(0.0, 0) 
	SCZH N 0 ACS_ExecuteAlways(501, 0)   
    stop
  XDeath:
	TNT1 A 0 A_GiveToTarget("ZombieKillXP", 1)
  XDeath.NoXP:
  XDeath.SentryTurret:
	TNT1 A 0 
	TNT1 A 0 Thing_ChangeTID(0, 0) //Remove BADDIES_TID so NightMorph won't resurrect us
 	TNT1 A 0 A_SpawnItemEx("ZombieScientistLoot", 0, 32, 0, 0, 0, 0, 0, 0, 0) 
    SCZH O 5
    SCZH P 5 A_XScream
    SCZH Q 5 A_NoBlocking
    SCZH RSTUV 5
    //SCZH W -1
	SCZH W 0 A_QueueCorpse //Queue the corpse for removal
	//Remove ourselves after 60 seconds
	SCZH W 2100
	TNT1 A 0 A_SetTranslucent(1.0, 2)
	SCZH W 6 A_SetTranslucent(0.8, 2)
	SCZH W 6 A_SetTranslucent(0.6, 2)
	SCZH W 6 A_SetTranslucent(0.4, 2)
	SCZH W 6 A_SetTranslucent(0.2, 2)
	SCZH W 6 A_SetTranslucent(0.0, 0) 
	SCZH W 0 ACS_ExecuteAlways(501, 0)   
    stop
  Raise:
    SCZH MLKJIH 5
    goto See
  Remove:
	TNT1 A 1 
	//Remove ourselves if we're too far from the player frustrum.
	TNT1 A 5 ACS_ExecuteAlways(501, 0)
	Stop
  }
}

ACTOR DoomZZombieScientist4
{
  Game Doom
  obituary "%o was put out by a zombie scientist."
  health 40
  mass 90
  speed 8
  Radius 20
  Height 52
  painchance 200
  seesound "grunt/sight"
  painsound "grunt/pain"
  deathsound "grunt/death"
  activesound "grunt/active"
  MeleeRange 128
  MONSTER
  +FLOORCLIP
  +WINDTHRUST
  Translation "80:95=128:143", "96:111=76:79"
  States
  {
   Spawn:
 	TNT1 A 35 //Let the monster drop to the ground since we spawn
			  //them way above ground level so they don't get stuck
			  //in the floor.
	TNT1 A 0 A_SetTranslucent(0.0, 2)
	SCFE N 6 A_SetTranslucent(0.2, 2)
	SCFE N 6 A_SetTranslucent(0.4, 2)
	SCFE N 6 A_SetTranslucent(0.6, 2)
	SCFE N 6 A_SetTranslucent(0.8, 2)
	SCFE N 6 A_SetTranslucent(1.0, 0) //opaque
	SCFE NMLKJIH 7 //Play death animation in reverse so it looks
	              //like it's coming out of the ground
	//Continue to next state
   Idle:
   SpawnLook:
	SCFE AABBCCDD 4 A_Wander
    SCFE A 0 A_LookEx(0, 0, 2000.0, 3000.0, 0, "See")
	SCFE AABBCCDD 4 A_Wander
    SCFE A 0 A_LookEx(0, 0, 2000.0, 3000.0, 0, "See")
	SCFE AABBCCDD 4 A_Wander
    SCFE A 0 A_LookEx(0, 0, 2000.0, 3000.0, 0, "See")
	SCFE AABBCCDD 4 A_Wander
    SCFE A 0 A_LookEx(0, 0, 2000.0, 3000.0, 0, "See")
	SCFE AABBCCDD 4 A_Wander
    SCFE A 0 A_LookEx(0, 0, 2000.0, 3000.0, 0, "See")
	SCFE AABBCCDD 4 A_Wander
    SCFE A 0 A_LookEx(0, 0, 2000.0, 3000.0, 0, "See")
	SCFE AABBCCDD 4 A_Wander
    SCFE A 0 A_LookEx(0, 0, 2000.0, 3000.0, 0, "See")
	SCFE AABBCCDD 4 A_Wander
    SCFE A 0 A_LookEx(0, 0, 2000.0, 3000.0, 0, "See")
	SCFE A 0 A_CheckSightOrRange(ScientistRemovalDistance, "Remove")
    loop
  See:
 	//Randomly decide to attack your base or not
	TNT1 A 0 A_Jump(200, 2) //200/256 chance of not changing state
	TNT1 A 0 ACS_ExecuteAlways(502, 0)
	//Only check to remove ourselves and change attack modes every 6 seconds
    SCFE AABBCCDD 4 A_Chase
    SCFE AABBCCDD 4 A_Chase
    SCFE AABBCCDD 4 A_Chase
    SCFE AABBCCDD 4 A_Chase
    SCFE AABBCCDD 4 A_Chase
    SCFE AABBCCDD 4 A_Chase
	SCFE A 0 A_CheckSightOrRange(ScientistRemovalDistance, "Remove")
    loop
  Melee:
    SCFE E 1 A_FaceTarget
    SCFE E 1 A_PlaySound ("Steam/Fire")
	/* Not supported in Zandronum 1.2 -- Albert
    SCFE F 1 A_CustomMissile ("HalonParticle", 32, 8, (0.1)*Random(-3, 3), CMF_OFFSETPITCH, (0.1)*Random(-3, 3))
    SCFE F 1 A_CustomMissile ("DummyHalonParticle", 32, 8, (0.1)*Random(-3, 3), CMF_OFFSETPITCH, (0.1)*Random(-3, 3))
    SCFE F 1 A_CustomMissile ("DummyHalonParticle", 32, 8, (0.1)*Random(-3, 3), CMF_OFFSETPITCH, (0.1)*Random(-3, 3))
    SCFE F 1 A_CustomMissile ("DummyHalonParticle", 32, 8, (0.1)*Random(-3, 3), CMF_OFFSETPITCH, (0.1)*Random(-3, 3))
    SCFE F 1 A_CustomMissile ("HalonParticle", 32, 8, (0.1)*Random(-3, 3), CMF_OFFSETPITCH, (0.1)*Random(-3, 3))
    SCFE F 1 A_CustomMissile ("DummyHalonParticle", 32, 8, (0.1)*Random(-3, 3), CMF_OFFSETPITCH, (0.1)*Random(-3, 3))
    SCFE F 1 A_CustomMissile ("DummyHalonParticle", 32, 8, (0.1)*Random(-3, 3), CMF_OFFSETPITCH, (0.1)*Random(-3, 3))
    SCFE F 1 A_CustomMissile ("DummyHalonParticle", 32, 8, (0.1)*Random(-3, 3), CMF_OFFSETPITCH, (0.1)*Random(-3, 3))
    */
	SCFE F 1 A_CustomMissile ("HalonParticle", 32, 8, (0.1)*Random(-3, 3), 0, (0.1)*Random(-3, 3))
    SCFE F 1 A_CustomMissile ("DummyHalonParticle", 32, 8, (0.1)*Random(-3, 3), 0, (0.1)*Random(-3, 3))
    SCFE F 1 A_CustomMissile ("DummyHalonParticle", 32, 8, (0.1)*Random(-3, 3), 0, (0.1)*Random(-3, 3))
    SCFE F 1 A_CustomMissile ("DummyHalonParticle", 32, 8, (0.1)*Random(-3, 3), 0, (0.1)*Random(-3, 3))
    SCFE F 1 A_CustomMissile ("HalonParticle", 32, 8, (0.1)*Random(-3, 3), 0, (0.1)*Random(-3, 3))
    SCFE F 1 A_CustomMissile ("DummyHalonParticle", 32, 8, (0.1)*Random(-3, 3), 0, (0.1)*Random(-3, 3))
    SCFE F 1 A_CustomMissile ("DummyHalonParticle", 32, 8, (0.1)*Random(-3, 3), 0, (0.1)*Random(-3, 3))
    SCFE F 1 A_CustomMissile ("DummyHalonParticle", 32, 8, (0.1)*Random(-3, 3), 0, (0.1)*Random(-3, 3))
    goto See
  Pain:
    SCFE G 3
    SCFE G 3 A_Pain
    goto See
  NightMorph:
    TNT1 A 0
	TNT1 A 0 A_Scream
	TNT1 A 0 A_NoBlocking
	SCFE HIJKLMN 4 
	TNT1 A 0 A_SpawnItemEx("ZombieIncarnate")
	stop
  Death:
	TNT1 A 0 A_GiveToTarget("ZombieKillXP", 1)
  Death.NoXP:
  Death.SentryTurret:
	TNT1 A 0 
	TNT1 A 0 Thing_ChangeTID(0, 0) //Remove BADDIES_TID so NightMorph won't resurrect us
    SCFE H 5
    SCFE I 5 A_Scream
    SCFE J 5 A_NoBlocking
    SCFE K 5
    SCFE L 5
    SCFE M 5
	TNT1 A 0 A_SpawnItemEx("ZombieScientistLoot", 0, 32, 0, 0, 0, 0, 0, 0, 0) 
    //SCFE N -1
	SCFE N 0 A_QueueCorpse //Queue the corpse for removal
	//Remove ourselves after 60 seconds
	SCFE N 2100
	TNT1 A 0 A_SetTranslucent(1.0, 2)
	SCFE N 6 A_SetTranslucent(0.8, 2)
	SCFE N 6 A_SetTranslucent(0.6, 2)
	SCFE N 6 A_SetTranslucent(0.4, 2)
	SCFE N 6 A_SetTranslucent(0.2, 2)
	SCFE N 6 A_SetTranslucent(0.0, 0) 
	SCFE N 0 ACS_ExecuteAlways(501, 0)   
    stop
  XDeath:
	TNT1 A 0 A_GiveToTarget("ZombieKillXP", 1)
  XDeath.NoXP:
  XDeath.SentryTurret:
	TNT1 A 0 
	TNT1 A 0 Thing_ChangeTID(0, 0) //Remove BADDIES_TID so NightMorph won't resurrect us
    SCFE O 5
    SCFE P 5 A_XScream
    SCFE Q 5 A_NoBlocking
    SCFE RSTUV 5
	TNT1 A 0 A_SpawnItemEx("ZombieScientistLoot", 0, 32, 0, 0, 0, 0, 0, 0, 0) 
    //SCFE W -1
	SCFE W 0 A_QueueCorpse //Queue the corpse for removal
	//Remove ourselves after 60 seconds
	SCFE W 2100
	TNT1 A 0 A_SetTranslucent(1.0, 2)
	SCFE W 6 A_SetTranslucent(0.8, 2)
	SCFE W 6 A_SetTranslucent(0.6, 2)
	SCFE W 6 A_SetTranslucent(0.4, 2)
	SCFE W 6 A_SetTranslucent(0.2, 2)
	SCFE W 6 A_SetTranslucent(0.0, 0) 
	SCFE W 0 ACS_ExecuteAlways(501, 0)   
    stop
  Raise:
    SCFE MLKJIH 5
    goto See
  Remove:
	TNT1 A 1 
	//Remove ourselves if we're too far from the player frustrum.
	TNT1 A 5 ACS_ExecuteAlways(501, 0)
	Stop
  }
}

