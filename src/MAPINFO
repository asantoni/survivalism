}

map DOOMZ1 "DoomZ 1"
{
   next = "DOOMZ2"
   sky1 = "SKY1"
   music = ""
   outsidefog = "00 00 01"
   OutsideFogDensity = "300"
   AllowRespawn //Allow the player to respawn in singleplayer
   ActivateOwnDeathSpecials //Needed for crafting boxes
}

