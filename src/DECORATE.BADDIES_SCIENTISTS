ACTOR DoomZFemaleScientist
{
  Game Doom
  obituary "%o was stabbed by a zombie scientist."
  health 20
  mass 90
  speed 10
  Radius 20
  Height 52
  painchance 200
  seesound "fem/sight"
  painsound "fem/pain"
  deathsound "fem/death"
  activesound "fem/active"
  MONSTER
  +FLOORCLIP
  +WINDTHRUST
  Translation "80:95=128:143", "96:111=76:79"
  States
  {
   Spawn:
 	TNT1 A 35 //Let the monster drop to the ground since we spawn
			  //them way above ground level so they don't get stuck
			  //in the floor.
	TNT1 A 0 A_SetTranslucent(0.0, 2)
	FSZK N 6 A_SetTranslucent(0.2, 2)
	FSZK N 6 A_SetTranslucent(0.4, 2)
	FSZK N 6 A_SetTranslucent(0.6, 2)
	FSZK N 6 A_SetTranslucent(0.8, 2)
	FSZK N 6 A_SetTranslucent(1.0, 0) //opaque
	FSZK NMLKJIH 7 //Play death animation in reverse so it looks
	              //like it's coming out of the ground
	//Continue to next state
   Idle:
   SpawnLook:
	FSZK AABBCCDD 4 A_Wander
    FSZK A 0 A_LookEx(0, 0, 2000.0, 3000.0, 0, "See")
	FSZK AABBCCDD 4 A_Wander
    FSZK A 0 A_LookEx(0, 0, 2000.0, 3000.0, 0, "See")
	FSZK AABBCCDD 4 A_Wander
    FSZK A 0 A_LookEx(0, 0, 2000.0, 3000.0, 0, "See")
	FSZK AABBCCDD 4 A_Wander
    FSZK A 0 A_LookEx(0, 0, 2000.0, 3000.0, 0, "See")
	FSZK AABBCCDD 4 A_Wander
    FSZK A 0 A_LookEx(0, 0, 2000.0, 3000.0, 0, "See")
	FSZK AABBCCDD 4 A_Wander
    FSZK A 0 A_LookEx(0, 0, 2000.0, 3000.0, 0, "See")
	FSZK AABBCCDD 4 A_Wander
    FSZK A 0 A_LookEx(0, 0, 2000.0, 3000.0, 0, "See")
	FSZK AABBCCDD 4 A_Wander
    FSZK A 0 A_LookEx(0, 0, 2000.0, 3000.0, 0, "See")
	FSZK A 0 A_CheckSightOrRange(ScientistRemovalDistance, "Remove")
    loop
  See:
 	//Randomly decide to attack your base or not
	TNT1 A 0 A_Jump(200, 2) //200/256 chance of not changing state
	TNT1 A 0 ACS_ExecuteAlways(502, 0)
	//Only check to remove ourselves and change attack modes every 6 seconds
    FSZK AABBCCDD 4 A_Chase
    FSZK AABBCCDD 4 A_Chase
    FSZK AABBCCDD 4 A_Chase
    FSZK AABBCCDD 4 A_Chase
    FSZK AABBCCDD 4 A_Chase
    FSZK AABBCCDD 4 A_Chase
	FSZK A 0 A_CheckSightOrRange(ScientistRemovalDistance, "Remove")
    loop
  Melee:
    TNT1 A 0 
    TNT1 A 0 A_JumpIfInventory("bananas", 1, "MeleeBananas")
    FSZK E 4 A_FaceTarget
    FSZK F 8 A_CustomMeleeAttack (random (1, 5) *5, "KnifeHit", "skeleton/swing")
    FSZK E 4
    goto See
  MeleeBananas:
    FSZK E 4 A_FaceTarget
    FSZK F 4 A_CustomMeleeAttack (random (1, 5) *5, "KnifeHit", "skeleton/swing")
    FSZK E 4
    FSZK F 8 A_CustomMeleeAttack (random (1, 5) *5, "KnifeHit", "skeleton/swing")
    FSZK E 4
    FSZK F 8 A_CustomMeleeAttack (random (1, 5) *5, "KnifeHit", "skeleton/swing")
    FSZK E 4
    FSZK F 4 A_CustomMeleeAttack (random (1, 5) *5, "KnifeHit", "skeleton/swing")
    FSZK E 4
    goto See
    /* XXX(STAGGER):
  Pain.Fist:
    TNT1 A 0
    TNT1 A 0 A_JumpIfInventory("Staggered", 1, "StaggerDeath")
    */
  Pain:
    FSZK G 3
    FSZK G 3 A_Pain
    goto See
    /* XXX(STAGGER):
  Pain.Bullet:
    FSZK G 0
    TNT1 A 0 A_JumpIfInventory("Staggered", 1, "Stagger")
    TNT1 A 0 A_Jump(200, "Pain")
  Stagger:
    TNT1 A 0 A_GiveInventory("Staggered", 1)
    TNT1 A 5
    FSZK G 35 A_Pain
    TNT1 A 5
    FSZK G 35 A_Pain
    TNT1 A 5
    FSZK G 35 A_Pain
    TNT1 A 0 A_TakeInventory("Staggered", 1)
    goto See
    
  StaggerDeath:
    TNT1 A 0
    TNT1 A 0 A_SpawnItemEx("DoomZHealthBonus", 0, 0, 0, random(2.0, 4.0), random(2.0, 4.0), random(2.0, 4.0))
    TNT1 A 0 A_SpawnItemEx("DoomZHealthBonus", 0, 0, 0, random(2.0, 4.0), random(2.0, 4.0), random(2.0, 4.0))
    TNT1 A 0 A_SpawnItemEx("DoomZHealthBonus", 0, 0, 0, random(2.0, 4.0), random(2.0, 4.0), random(2.0, 4.0))
    TNT1 A 0 A_SpawnItemEx("DoomZHealthBonus", 0, 0, 0, random(2.0, 4.0), random(2.0, 4.0), random(2.0, 4.0))
    TNT1 A 0 A_Die
    */
  NightMorph:
    TNT1 A 0
	TNT1 A 0 A_Scream
	TNT1 A 0 A_NoBlocking
	FSZK HIJKLMN 4 
	TNT1 A 0 A_SpawnItemEx("ZombieIncarnate")
	stop
  Death:
	TNT1 A 0 A_GiveToTarget("ZombieKillXP", 1)
  Death.NoXP:
  Death.SentryTurret:
	TNT1 A 0 
	TNT1 A 0 Thing_ChangeTID(0, 0) //Remove BADDIES_TID so NightMorph won't resurrect us
    FSZK H 5 
    FSZK I 5 A_Scream
    FSZK J 5 A_NoBlocking
    FSZK K 5
    FSZK L 5
    FSZK M 5
	TNT1 A 0 A_SpawnItemEx("ZombieScientistLoot", 0, 32, 0, 0, 0, 0, 0, 0, 0)
	FSZK N 0 A_QueueCorpse //Queue the corpse for removal
	//Remove ourselves after 60 seconds
	FSZK N 2100
	TNT1 A 0 A_SetTranslucent(1.0, 2)
	FSZK N 6 A_SetTranslucent(0.8, 2)
	FSZK N 6 A_SetTranslucent(0.6, 2)
	FSZK N 6 A_SetTranslucent(0.4, 2)
	FSZK N 6 A_SetTranslucent(0.2, 2)
	FSZK N 6 A_SetTranslucent(0.0, 0) 
	FSZK N 0 ACS_ExecuteAlways(501, 0)    
    stop
  XDeath:
	TNT1 A 0 A_GiveToTarget("ZombieKillXP", 1)
  Death.NoXP:
  Death.SentryTurret:
    FSZK O 5
    FSZK P 5 A_XScream
    FSZK Q 5 A_NoBlocking
    FSZK RSTUV 5
    FSZK W 5
	TNT1 A 0 A_SpawnItemEx("ZombieScientistLoot", 0, 32, 0, 0, 0, 0, 0, 0, 0)
	FSZK W 0 A_QueueCorpse //Queue the corpse for removal
	//Remove ourselves after 60 seconds
	FSZK W 2100
	TNT1 A 0 A_SetTranslucent(1.0, 2)
	FSZK W 6 A_SetTranslucent(0.8, 2)
	FSZK W 6 A_SetTranslucent(0.6, 2)
	FSZK W 6 A_SetTranslucent(0.4, 2)
	FSZK W 6 A_SetTranslucent(0.2, 2)
	FSZK W 6 A_SetTranslucent(0.0, 0) 
	FSZK W 0 ACS_ExecuteAlways(501, 0)    
    stop
  Raise:
    FSZK MLKJIH 5
    goto See
  Remove:
	TNT1 A 0 
	//Remove ourselves if we're too far from the player frustrum.
	TNT1 A 5 ACS_ExecuteAlways(501, 0)
	Stop
  }
}

actor ScientistCorpse {

}

actor DoomZFemaleScientistCorpse : ScientistCorpse 
{
	+SHOOTABLE
	Mass 0x7FFFFFFF
	Health 150
	states {
		Spawn:
			//Remove ourselves after 60 seconds
			FSZK W 2100
			TNT1 A 0 A_SetTranslucent(1.0, 2)
			FSZK W 6 A_SetTranslucent(0.8, 2)
			FSZK W 6 A_SetTranslucent(0.6, 2)
			FSZK W 6 A_SetTranslucent(0.4, 2)
			FSZK W 6 A_SetTranslucent(0.2, 2)
			FSZK W 6 A_SetTranslucent(0.0, 0) 
			FSZK W 0 ACS_ExecuteAlways(501, 0)    
			stop
	}
}

