	TNT1 A 0 A_SetTranslucent(1.0, 2)
	HMZP L 6 A_SetTranslucent(0.8, 2)
	HMZP L 6 A_SetTranslucent(0.6, 2)
	HMZP L 6 A_SetTranslucent(0.4, 2)
	HMZP L 6 A_SetTranslucent(0.2, 2)
	HMZP L 6 A_SetTranslucent(0.0, 0) 
	HMZP L 0 ACS_ExecuteAlways(501, 0)   
    stop
  XDeath:
	TNT1 A 0 A_GiveToTarget("ZombieKillXP", 1)
  XDeath.NoXP:
