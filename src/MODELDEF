Model CBox
{
    Path "MODELS"  // Path to model in PK3
    Model 0 "box.md2" // Model index, model file
    Skin 0 "box3.png"  // Model index, texture 
	Scale 12.5 12.5 12.5    // Scale values
	ZOffset 23 //Needed this when I inverted Z scale

    FrameIndex CBOX A 0 0 // The sprite lump, sprite frame, model index, frame number
}

